generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id          Int     @id @default(autoincrement())
  title       String
  category    String
  description String
  material    String
  dimensions  String
  finish      String?
  duration    String?
  badge       String?
  imageLabel  String
  images      String[] @default([])
}
